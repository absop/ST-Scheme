[
    {
        "id": "preferences",
        "children": [
            {
                "caption": "Package Settings",
                "id": "package-settings",
                "mnemonic": "P",
                "children": [
                    {
                        "caption": "Schemer",
                        "mnemonic": "S",
                        "children": [
                            {
                                "caption": "README",
                                "command": "open_file",
                                "args": {
                                    "file": "${packages}/Schemer/README.md"
                                }
                            },
                            {
                                "caption": "-"
                            },
                            {
                                "caption": "Key Bindings",
                                "command": "edit_settings",
                                "mnemonic": "K",
                                "args": {"base_file": "${packages}/Schemer/keymap/Default ($platform).sublime-keymap", "default": "[\n\t$0\n]\n"}
                            }
                        ]
                    }
                ]
            }
        ]
    }
]
